{
  "name": "dschuppelius/php-financial-formats",
  "description": "PHP library for parsing, validating and processing financial and banking file formats such as CAMT, MT and PAIN.",
  "type": "library",
  "license": "AGPL-3.0-or-later",
  "authors": [
    {
      "name": "Daniel Joerg Schuppelius",
      "email": "info@schuppelius.org"
    }
  ],
  "require": {
    "php": ">=8.2 <8.5",
    "dschuppelius/php-common-toolkit": "^0.9.1"
  },
  "autoload": {
    "psr-4": {
      "CommonToolkit\\FinancialFormats\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^11.3"
  },
  "prefer-stable": true,
  "minimum-stability": "stable",
  "config": {
    "sort-packages": true,
    "optimize-autoloader": true
  },
  "scripts": {
    "test": "vendor/bin/phpunit"
  },
  "repositories": {
    "local-common-toolkit": {
      "type": "path",
      "url": "../php-common-toolkit",
      "options": {
        "symlink": true
      }
    }
  }
}
